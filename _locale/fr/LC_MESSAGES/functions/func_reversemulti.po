#: ../../functions/func_reversemulti.rst:8
msgid "Usage: **$reversemulti(name[,separator])**"
msgstr "Utilisation: **$reversemulti(nom[,séparateur])**"

#: ../../functions/func_reversemulti.rst:9
msgid "Category: multi-value"
msgstr "Catégorie: multi-valeur"

#: ../../functions/func_reversemulti.rst:10
msgid "Implemented: Picard 2.3.1"
